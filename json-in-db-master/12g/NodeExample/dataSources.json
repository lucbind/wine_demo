{
  "useProxy": false,
  "proxy": {
    "hostname": null,
    "port": null
  },
  "tmdb": {
    "protocol": "http",
    "hostname": "api.themoviedb.org",
    "port": 80,
    "apiPath": "/3",
    "searchCriteria": {
      "language": "en",
      "country": "US",
      "certification": "PG-13",
      "popularity": 2,
      "releaseDates": {
        "start": "2013-01-01",
        "end": "2018-01-30"
      },
      "movieLimit": 1024
    },
    "apiKey": "YOUR_TMDB_KEY_GOES_HERE"
  },
  "fandango": {
    "protocol": "https",
    "hostname": "www.fandango.com",
    "port": 443,
    "path": "/rss/moviesnearme_",
    "searchCriteria": {
      "zipCode": "94501"
    }
  },
  "mappingService": "none",
  "usCensus": {
    "protocol": "http",
    "hostname": "geocoding.geo.census.gov",
    "port": 80,
    "path": "/geocoder/locations/onelineaddress",
    "benchmarks": [
      "Public_AR_Current",
      "Public_AR_ACS2017",
      "Public_AR_Census2010"
    ]
  },
  "google": {
